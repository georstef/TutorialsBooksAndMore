<Window x:Class="MDIexample.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:syncfusion="http://schemas.syncfusion.com/wpf"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MDIexample"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <syncfusion:DockingManager Margin="5,32,5,30" 
                                   x:Name="dmnMDIPanel" 
                                   UseDocumentContainer="True" 
                                   ContainerMode="MDI" 
                                   ShowFloatWindowInTaskbar="True"
                                   Background="DarkGray">

        </syncfusion:DockingManager>
        <Button x:Name="button" Content="w1" HorizontalAlignment="Left" Margin="14,12,0,0" VerticalAlignment="Top" Width="75" Click="button_Click_1"/>
        <Button x:Name="button1" Content="w2" HorizontalAlignment="Left" Margin="128,12,0,0" VerticalAlignment="Top" Width="75" Click="button1_Click"/>
    </Grid>
</Window>
